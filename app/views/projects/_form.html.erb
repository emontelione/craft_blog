<%= form_for :project, url: projects_path do |f| %>
  <% if @project.errors.any? %>
  <div>
    <% @project.errors.full_messages.each do |error_message| %>
      <p><%= error_message %></p>
    <% end %>
  </div>
  <% end %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>

  <p>
    <%= f.fields_for :images do |p| %>
      <%= p.label :filename %>
      <%= p.text_field :filename %>
    <% end %>
  </p>

  <%= f.submit %>
  </p>
<% end %>